{
  "kind": "collectionType",
  "collectionName": "thinkify_candidates",
  "info": {
    "name": "thinkify_candidates",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email_id": {
      "type": "email",
      "required": true,
      "unique": true,
      "minLength": 4
    },
    "profile_photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "phone_number": {
      "type": "string"
    },
    "resume": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "github": {
      "type": "string"
    },
    "linkedIn": {
      "type": "string"
    },
    "notice_period": {
      "type": "string"
    },
    "current_salary": {
      "type": "decimal"
    },
    "expected_salary": {
      "type": "decimal"
    },
    "designation": {
      "type": "string"
    },
    "myself_160": {
      "type": "text",
      "maxLength": 160
    },
    "offers_from": {
      "collection": "company-list"
    },
    "previously_worked_companies": {
      "collection": "company-list"
    },
    "current_working_company": {
      "model": "company-list"
    },
    "t_feedback": {
      "via": "thinkify_candidate",
      "model": "t-feedback"
    },
    "skills": {
      "collection": "skills",
      "via": "thinkify_candidates",
      "dominant": true
    },
    "my_works": {
      "type": "component",
      "repeatable": true,
      "component": "my-work.my-work"
    }
  }
}
