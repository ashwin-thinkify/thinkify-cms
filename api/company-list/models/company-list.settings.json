{
  "kind": "collectionType",
  "collectionName": "company_lists",
  "info": {
    "name": "company_list"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "website": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "company": {
      "via": "current_working_company",
      "model": "candidates"
    },
    "companies": {
      "via": "previously_worked_companies",
      "collection": "candidates"
    },
    "current_working_companies": {
      "via": "current_working_company",
      "model": "interviewer"
    }
  }
}
